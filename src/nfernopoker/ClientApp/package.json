    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/webpack/test.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "\\.spec\\.tsx$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\index.tsx": "<rootDir/__mocks__/index.js>"
    },
    "unmockedModulePathPatterns": [
      "./node_modules/react/"
    ]
  },
  "name": "nfernopoker",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=development&&webpack-dev-server --port 8081 --config ./webpack/webpack.dev.js --color --progress --open",
    "build": "set NODE_ENV=production&&webpack --config ./webpack/webpack.prod.js --color --progress",
    "test": "jest",
    "test:watch": "jest --watch",
    "eject": "react-scripts-ts eject"
  },
  "version": "0.1.0"
}
